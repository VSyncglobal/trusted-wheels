{
  "name": "trusted-wheels",
  "private": true,
  "packageManager": "npm@10.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:push": "cd packages/database && npx prisma db push",
    "db:studio": "cd packages/database && npx prisma studio"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "turbo": "^2.7.2"
  },
  "dependencies": {
    "use-debounce": "^10.0.6"
  }
}
